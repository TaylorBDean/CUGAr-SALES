{
  "dashboard": {
    "id": null,
    "uid": "cuga-agent-dashboard",
    "title": "CUGAR Agent System - Golden Signals",
    "tags": ["cuga", "agent", "observability"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "templating": {
      "list": [
        {
          "name": "profile",
          "type": "query",
          "datasource": "${DS_PROMETHEUS}",
          "query": "label_values(cuga_requests_total, profile)",
          "current": {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Success Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "cuga_success_rate",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Tool Error Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "cuga_tool_error_rate",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 15, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Mean Steps Per Task",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "cuga_steps_per_task",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Budget Utilization",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "cuga_budget_warnings_total",
            "legendFormat": "Warnings",
            "refId": "A"
          },
          {
            "expr": "cuga_budget_exceeded_total",
            "legendFormat": "Exceeded",
            "refId": "B"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 5,
        "title": "Request Rate (req/s)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(cuga_requests_total[1m])",
            "legendFormat": "Total",
            "refId": "A"
          },
          {
            "expr": "rate(cuga_requests_success_total[1m])",
            "legendFormat": "Success",
            "refId": "B"
          },
          {
            "expr": "rate(cuga_requests_failed_total[1m])",
            "legendFormat": "Failed",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 6,
        "title": "End-to-End Latency",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(cuga_latency_ms_bucket[1m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cuga_latency_ms_bucket[1m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(cuga_latency_ms_bucket[1m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Tool Call Latency by Tool",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cuga_tool_latency_ms_bucket[1m]))",
            "legendFormat": "{{tool}} P95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Approval Wait Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(cuga_approval_wait_ms_bucket[1m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cuga_approval_wait_ms_bucket[1m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(cuga_approval_wait_ms_bucket[1m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5000, "color": "yellow"},
                {"value": 15000, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Tool Errors by Type",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum by (error_type) (rate(cuga_tool_errors_by_type[5m]))",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 10,
        "title": "Tool Errors by Tool",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
        "targets": [
          {
            "expr": "sum by (tool) (rate(cuga_tool_errors_by_tool[5m]))",
            "legendFormat": "{{tool}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 11,
        "title": "Active Traces",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
        "targets": [
          {
            "expr": "count(count by (trace_id) (cuga_requests_total))",
            "refId": "A"
          }
        ],
        "options": {
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      },
      {
        "id": 12,
        "title": "Recent Events Timeline",
        "type": "logs",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "{job=\"cuga-agent\"} |= \"event_type\"",
            "refId": "A"
          }
        ],
        "options": {
          "showLabels": true,
          "showTime": true,
          "wrapLogMessage": true,
          "sortOrder": "Descending"
        }
      }
    ]
  }
}
